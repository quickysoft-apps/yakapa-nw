{
  "name": "@yakapa/core",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "neurosoup <neurosoup@gmail.com>",
  "homepage": "https://github.com/quickysoft-apps/yakapa-nw#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quickysoft-apps/yakapa-nw.git"
  },
  "main": "http://localhost:8080/index.html",
  "node-remote": [
    "*://*/*"
  ],
  "window": {
    "width": 800,
    "height": 600,
    "frame": true
  },
  "chromium-args": "--enable-logging=stderr --load-extension=../../extensions/react-devtools_3.6.0_0,../../extensions/settings",
  "scripts": {
    "start-parcel": "../../node_modules/.bin/parcel index.html -p 8080 --no-autoinstall",
    "start-nw": "run --x64 --mirror https://dl.nwjs.io/ .",
    "start": "concurrently --kill-others \"npm:start-*\"",
    "build": "build --tasks mac-x64 --mirror https://dl.nwjs.io/ .",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "build": {
    "nwVersion": "0.36.2",
    "overriddenProperties": {
      "chromium-args": "--load-extension=../../extensions/settings"
    },
    "output": "bin/",
    "files": "**/*"
  },
  "devDependencies": {
    "@types/chrome": "0.0.81",
    "@types/nw.gui": "0.0.20",
    "@types/nw.js": "^0.13.8",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "nwjs-builder-phoenix": "^1.15.0",
    "wait-on": "^3.2.0"
  },
  "dependencies": {}
}
