{
  "name": "@extensions/settings",
  "version": "0.0.0",
  "description": "Core settings",
  "author": "neurosoup <neurosoup@gmail.com>",
  "homepage": "https://github.com/quickysoft-apps/yakapa-nw#readme",
  "license": "ISC",
  "main": "build/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quickysoft-apps/yakapa-nw.git"
  },
  "scripts": {
    "start:parcel": "../../node_modules/.bin/parcel watch src/**/* --out-dir ../core/dist/extensions/settings/build --no-autoinstall --no-hmr",
    "start:extension": "../../node_modules/.bin/cpx \"manifest.json\" \"../core/dist/extensions/settings\"",
    "start": "../../node_modules/.bin/npm-run-all -lp start:*",
    "deploy-src": "../../node_modules/.bin/parcel build src/*.ts src/*.tsx -d ../core/dist/extensions/settings/build",
    "deploy-manifest": "../../node_modules/.bin/cpx \"manifest.json\" \"../core/dist/extensions/settings\"",
    "deploy": "../../node_modules/.bin/npm-run-all -lp deploy:*",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.81"
  }
}
